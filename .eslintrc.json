{
  "root": true,
  "env": {
    "browser": false,
    "node": true,
    "es2021": true,
    "jest": false
  },
  "extends": ["airbnb-base", "plugin:import/recommended"],
  "plugins": ["import"],
  "rules": {
    "indent": ["error", 2],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "import/no-unresolved": [2, {
      "commonjs": true,
      "amd": true
      }],
    "import/named": 2,
    "import/namespace": 2,
    "import/default": 2,
    "import/export": 2
  },
  "settings": {
    "import/extensions": [
      ".js"
    ]
  }
}
